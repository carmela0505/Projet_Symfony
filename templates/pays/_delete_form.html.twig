<form method="post" action="{{ path('app_pays_delete', {'idPays': pay.idPays}) }}" onsubmit="return confirm('{% trans %} Souhaitez-vous supprimer ce pays?{% endtrans %}'); ">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ pay.idPays) }}">
     <button class="btn btn-danger">{% trans %}Supprimer{% endtrans %}</button>
</form>
