<form method="post" action="{{ path('app_ticket_delete', {'annee': ticket.annee}) }}" onsubmit="return confirm('{% trans %} Souhaitez-vous supprimer ce ticket?{% endtrans %}'); ">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ ticket.annee) }}">
     <button class="btn btn-danger">{% trans %}Supprimer{% endtrans %}</button>
</form>
